--
-- TOC entry 221 (class 1255 OID 16432)
-- Name: delete_blocked_user(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.delete_blocked_user() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
    IF NEW.user_state_id = (
        SELECT id FROM public.user_state WHERE code = 'Blocked'
    ) THEN
        DELETE FROM public."user" WHERE id = OLD.id;
    END IF;
    RETURN NEW;
END;
$$;


ALTER FUNCTION public.delete_blocked_user() OWNER TO postgres;

--
-- TOC entry 220 (class 1255 OID 16468)
-- Name: set_user_defaults(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.set_user_defaults() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
  NEW.user_state_id := 1;
  NEW.created_date := CURRENT_TIMESTAMP;
  RETURN NEW;
END;
$$;


ALTER FUNCTION public.set_user_defaults() OWNER TO postgres;

